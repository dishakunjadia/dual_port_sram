    %pad/u 7;
    %ix/vec4 3;
    %ix/load 4, 8, 0; part off
    %ix/load 5, 0, 0; Constant delay
    %assign/vec4/a/d v0x1506fa0, 4, 5;
T_0.6 ;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x14eb220_0;
    %pad/u 7;
