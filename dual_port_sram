v0x14eb220_0 .net "addr_a", 4 0, v0x1507380_0;  1 drivers
v0x15066e0_0 .net "addr_b", 4 0, v0x1507460_0;  1 drivers
v0x15067c0_0 .net "byte_en_a", 1 0, v0x1507530_0;  1 drivers
v0x15068b0_0 .net "byte_en_b", 1 0, v0x1507630_0;  1 drivers
