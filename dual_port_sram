    %assign/vec4 v0x1506c60_0, 0;
T_0.3 ;
T_0.0 ;
    %load/vec4 v0x1506ee0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.8, 8;
    %load/vec4 v0x1507120_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.10, 8;
    %load/vec4 v0x15068b0_0;
