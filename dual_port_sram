    %flag_set/vec4 8;
    %jmp/0xz  T_0.14, 8;
    %load/vec4 v0x1506b80_0;
    %parti/s 8, 8, 5;
    %load/vec4 v0x15066e0_0;
    %pad/u 7;
    %ix/vec4 3;
    %ix/load 4, 8, 0; part off
    %ix/load 5, 0, 0; Constant delay
    %assign/vec4/a/d v0x1506fa0, 4, 5;
T_0.14 ;
    %jmp T_0.11;
T_0.10 ;
    %load/vec4 v0x15066e0_0;
    %pad/u 7;
    %ix/vec4 4;
    %load/vec4a v0x1506fa0, 4;
    %assign/vec4 v0x1506d40_0, 0;
T_0.11 ;
T_0.8 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x14eb090;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1507700_0, 0, 1;
    %end;
    .thread T_1, $init;
    .scope S_0x14eb090;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x1507700_0;
    %inv;
    %store/vec4 v0x1507700_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x14eb090;
T_3 ;
    %vpi_call/w 3 25 "$dumpfile", "sim/dual_port_sram.vcd" {0 0 0};
    %vpi_call/w 3 26 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x14eb090 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1507d10_0, 0, 1;
