v0x1506d40_0 .var "dout_b", 15 0;
v0x1506e20_0 .net "en_a", 0 0, v0x1507b70_0;  1 drivers
v0x1506ee0_0 .net "en_b", 0 0, v0x1507c40_0;  1 drivers
v0x1506fa0 .array "mem", 31 0, 15 0;
v0x1507060_0 .net "we_a", 0 0, v0x1507d10_0;  1 drivers
v0x1507120_0 .net "we_b", 0 0, v0x1507de0_0;  1 drivers
E_0x14bfc40 .event posedge, v0x1506990_0;
    .scope S_0x14ace60;
T_0 ;
    %wait E_0x14bfc40;
    %load/vec4 v0x1506e20_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x1507060_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x15067c0_0;
    %parti/s 1, 0, 2;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.4, 8;
    %load/vec4 v0x1506aa0_0;
    %parti/s 8, 0, 2;
    %load/vec4 v0x14eb220_0;
    %pad/u 7;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x1506fa0, 0, 4;
T_0.4 ;
    %load/vec4 v0x15067c0_0;
    %parti/s 1, 1, 2;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.6, 8;
